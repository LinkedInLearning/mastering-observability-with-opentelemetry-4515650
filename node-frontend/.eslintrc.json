{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier", "import"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1,
      "no-underscore-dangle": 0,
      "no-undef": 1,
      "import/no-extraneous-dependencies": 0
    }
}